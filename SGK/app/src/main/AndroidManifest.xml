        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.SGK.sgk.DeliveryProducts"></activity>
        <activity android:name="com.SGK.sgk.UndispatchOrder" />
        <activity android:name="com.SGK.sgk.ErrorConnection" />
        <activity
