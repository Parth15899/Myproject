        <activity android:name="com.SGK.sgk.Terms" />
        <activity android:name="com.SGK.sgk.ConfirmOrder" />
        <activity android:name="com.SGK.sgk.AddAddress" />
        <activity android:name="com.SGK.sgk.SubCategory" />
        <activity android:name="com.SGK.sgk.searchAct" />
