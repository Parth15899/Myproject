            android:name="com.SGK.sgk.verifyfpotp"
            android:theme="@style/noActionBar" />
        <activity
            android:name="com.SGK.sgk.fpass"
            android:theme="@style/noActionBar" />
        <activity
            android:name="com.SGK.sgk.Login"
            android:theme="@style/noActionBar" />
        <activity
            android:name="com.SGK.sgk.SignUp"
            android:theme="@style/noActionBar" />
        <activity android:name="com.SGK.sgk.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>